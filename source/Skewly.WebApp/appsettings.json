{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Redis": {
    "Configuration": "localhost:6379",
    "InstanceName": "skewly-api-data"
  },
  "Elasticsearch": {
    "Server": "http://localhost:9200"
  },
  "ClientRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "HttpStatusCode": 429
  },
  "AllowedHosts": "*"
}
